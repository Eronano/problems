{
    "files.associations": {
        "**/include/c++/**": "cpp"
    },
    "markdownlint.config": {
        "MD013": false,
        "MD025": false,
        "MD033": false
    },
    "python.analysis.diagnosticSeverityOverrides": {
        "reportWildcardImportFromLibrary": "none"
    },
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "120"
    ]
}